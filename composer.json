{
	"name": "rackbeat/php-throttler",
	"description": "Throttle PHP function calls and iteration runs by X executions per second.",
	"keywords": [
		"laravel",
		"php",
		"throttle",
		"rate-limit"
	],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Lasse Rafn",
			"email": "lasserafn@gmail.com"
		},
		{
			"name": "Rackbeat",
			"email": "open-source@rackbeat.com"
		}
	],
	"require": {
		"php": ">=7.3"
	},
	"require-dev": {
		"phpunit/phpunit": "^7.0",
		"php-coveralls/php-coveralls": "^1.0"
	},
	"autoload": {
		"files": [
			"src/functions.php"
		],
		"psr-4": {
			"Rackbeat\\Throttler\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Throttler\\Tests\\": "tests/"
		}
	},
	"config": {
		"sort-packages": true
	}
}
